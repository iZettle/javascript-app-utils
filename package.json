{
  "name": "@izettle/app-utils",
  "version": "0.6.0",
  "description": "Utilities and helpers for javascript applications developed with React, Redux and RxJS.",
  "main": "dist/index.js",
  "scripts": {
    "build": "`yarn bin`/babel src --presets babel-preset-es2015 --out-dir dist",
    "test": "`yarn bin`/javascript-env test",
    "lint": "`yarn bin`/javascript-env lint",
    "prepublish": "yarn run lint && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iZettle/javascript-app-utils.git"
  },
  "keywords": [
    "JavaScript",
    "React",
    "Redux",
    "RxJS"
  ],
  "author": "Martin Liberg",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/iZettle/javascript-app-utils/issues"
  },
  "homepage": "https://github.com/iZettle/javascript-app-utils#readme",
  "devDependencies": {
    "@izettle/javascript-env": "^3.3.5",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.15.0",
    "karma": "^1.4.1",
    "redux": "^3.6.0",
    "redux-observable": "^0.13.0",
    "rxjs": "^5.1.1"
  },
  "peerDependencies": {
    "redux": "^3.6.0",
    "redux-observable": "^0.13.0",
    "rxjs": "^5.1.1"
  }
